{
  "collectCoverage": true,
  "coverageDirectory": "./build/coverage/app",
  "coverageReporters": [
    "lcov",
    "text",
    "html"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 90,
      "branches": 90,
      "functions": 90,
      "lines": 90
    }
  },
  "globals": {
    "__DEV__": true
  },
  "globalSetup": "jest-preset-angular/global-setup",
  "preset": "jest-preset-angular/presets/defaults",
  "resetMocks": true,
  "resetModules": true,
  "roots": [
    "./src"
  ],
  "setupFiles": [
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/src/setup-jest.ts"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!.*\\.mjs$)"
  ]
}
