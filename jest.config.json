{
  "collectCoverage": true,
  "coverageDirectory": "./build/coverage/jest",
  "coverageReporters": [
    "lcovonly",
    "html",
    "json"
  ],
  "globals": {
    "__DEV__": true
  },
  "globalSetup": "jest-preset-angular/global-setup",
  "preset": "jest-preset-angular/presets/defaults",
  "resetMocks": true,
  "resetModules": true,
  "setupFiles": [
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/src/setup-jest.ts"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!.*\\.mjs$)"
  ],
  "testMatch": [
    "<rootDir>/src/**/*.spec.ts"
  ]
}
